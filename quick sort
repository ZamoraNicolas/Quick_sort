#include <stdlib.h>
#include <iostream>
#include <time.h>

using namespace std;

void arreglo();

int main()
{
arreglo();
}

void arreglo(){
    
int arreglo[20];
srand(time(NULL));

for (int i=0;i<20;i++){
    arreglo[i] = rand()%100;
}

for(int i=0;i<20;i++){
    std::cout << arreglo[i] << " " ;
}

}
